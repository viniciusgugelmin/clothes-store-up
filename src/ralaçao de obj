private Payments_methods_data instantiatePayments_methods_data (ResultSet rs, Payments_methods payMet) throws SQLException {
	Payments_methods_data obj = new Payments_methods_data();
	obj.setId(rs.getInt("Id"));
	obj.setPayments_methods_type (rs.getString("type"));
	obj.setCard_number(rs.getString("card_number"));
	obj.setExpiry_date(rs.getDate("Expiry_date"));
	obj.setCvc(rs.getInt("Cvc"));
	
	obj.setDepartment(payMet);
	return obj;
}

private payment_methods instantiatepayment_methods(ResultSet rs) throws SQLException {
payment_methods payMet = new Payment_methods();
payMet.setId(rs.getInt("payment_methodsId"));
payMet.setPayments_methods_type(rs.getString("payMetPayments_methods_type"));
return payMet;
}